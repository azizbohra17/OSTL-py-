    "def reverse(s): \n",
    "    return s[::-1] \n",
    "  \n",
    "def Palindrome(s):  \n",
    "    rev = reverse(s) \n",
    "\n",
    "    if (s == rev): \n",
    "        print(\"Palindrome\")\n",
    "    else:\n",
    "        print(\"Not a Palindrome\")\n",
    "   \n",
    "s = input(\"Enter a string\")\n",
    "Palindrome(s) \n",
    "  "
