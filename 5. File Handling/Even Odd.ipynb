    "f = open(\"file.txt\",\"r\")\n",
    "f1 = open(\"even.txt\",\"w\")\n",
    "f2 = open(\"odd.txt\",\"w\")\n",
    "for a in f:\n",
    " numbers = a.split()\n",
    "for s in numbers:\n",
    "  if int(s)%2 == 0:\n",
    "   f1.write(s+\" \")\n",
    "  else :\n",
    "   f2.write(s+\" \")"
   ]
